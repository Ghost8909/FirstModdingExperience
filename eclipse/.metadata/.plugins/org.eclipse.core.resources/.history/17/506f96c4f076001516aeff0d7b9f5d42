package com.ghost8909.firstmoddingexperience.blocks;

import net.minecraft.block.Block;
import net.minecraft.block.material.Material;

public class BlockCopper extends BlocksFME
{
	public BlockCopper()
	{
		super(Material.iron);
		this.setBlockName("blockCopper");
	}
	
	public static final Block.SoundType soundTypeMetal = new Block.SoundType("stone", 1.0F, 1.0F);
}
